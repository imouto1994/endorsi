{
  "manifest_version": 2,
  "name": "Endorsi",
  "version": "1.0.0",

  "description": "A personal extension from Youn",
  "icons": {
    "16": "icons/extension-16.png",
    "48": "icons/extension-48.png",
    "128": "icons/extension-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_page": "options.html",
  "permissions": ["storage"],
  "web_accessible_resources": ["*.jpg", "*.png"]
}
